<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.won.healthnavi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/health_navi_logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/health_navi_logo2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainListActivity" />
        <activity android:name=".ReservationActivity" />
        <activity android:name=".reservationListRecyclerView.ReservationChoiceTimeActivity"/>
    </application>

</manifest>